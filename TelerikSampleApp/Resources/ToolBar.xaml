                        </Grid>

                        <!-- Focus -->
                        <Border x:Name="CommonStatesWrapper">
                            <Border x:Name="FocusVisual" Visibility="Collapsed" Opacity="0" 
                                    CornerRadius="{TemplateBinding CornerRadius}" 
                                    BorderThickness="1" BorderBrush="{StaticResource MarkerBrush}"  />
                        </Border>

                    </Grid>

                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="CheckedVisual" Property="Opacity" Value="1" />
                            <Setter TargetName="Content" Property="Foreground" Value="{StaticResource MainBrush}" />
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="Border" Property="Background" Value="{StaticResource BasicBrush}" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="CommonStatesWrapper" Property="Opacity" Value="0" />
                            <Setter TargetName="Border" Property="Background" Value="{StaticResource BasicBrush}" />
                            <Setter TargetName="ContentGrid" Property="Margin" Value="0 0 0 -1" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Content" Property="Opacity" Value="0.5" />
                        </Trigger>
                        <Trigger Property="IsFocused" Value="True" >
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible" />
                            <Setter TargetName="FocusVisual" Property="Opacity" Value="1" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="telerik:RadDropDownButton" x:Key="ToolBarRadDropDownButton">
        <Setter Property="Margin" Value="1" />
        <Setter Property="Padding" Value="2" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Foreground" Value="{StaticResource StrongBrush}"/>
        <Setter Property="FontFamily" Value="Segoe UI" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadDropDownButton">
                    <Grid>
                        

                        <Border x:Name="Border" 
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Background="{TemplateBinding Background}"
								BorderBrush="{TemplateBinding BorderBrush}"
                                CornerRadius="{TemplateBinding CornerRadius}" />

                        <!-- checked -->
                        <Border x:Name="CheckedVisual" Opacity="0"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{StaticResource AccentBrush}"
                                Background="{StaticResource AccentBrush}"
                                CornerRadius="{TemplateBinding CornerRadius}" />
                        
                        <Grid SnapsToDevicePixels="True"
                             >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <ContentControl x:Name="Content" Grid.Row="1" Grid.Column="1"
                                    Foreground="{TemplateBinding Foreground}"
									Margin="{TemplateBinding Padding}"
									Content="{TemplateBinding Content}"
									ContentTemplate="{TemplateBinding ContentTemplate}"
									VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
									HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"/>
                            
                                <Path x:Name="DropDownIndicator" Grid.Row="2" Grid.Column="1"
										IsHitTestVisible="False"
										Visibility="{TemplateBinding DropDownIndicatorVisibility}"
										Fill="{StaticResource StrongBrush}"
										HorizontalAlignment="Center" VerticalAlignment="Center"
										RenderTransformOrigin="0.5,0.5" Margin="5,3,5,3"
										Data="M0,0 L5,0 5,1 4,1 4,2 3,2 3,3 2,3 2,2 1,2 1,1 0,1 0,0 z" />
                        </Grid>
                        <Popup x:Name="DropDownPopup"
							
                            AllowsTransparency="True" StaysOpen="False" Focusable="false" Placement="Bottom"
							IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}"
                            
                            >
                            <Grid>
                                <Border x:Name="DropDownPopupBorder"
										BorderBrush="{StaticResource ControlOuterBorder}"
										Background="{StaticResource ControlBackground}"
										BorderThickness="1"
										CornerRadius="{TemplateBinding CornerRadius}"
										Margin="0" MinWidth="3" MinHeight="3"
										Width="{TemplateBinding DropDownWidth}"
										Height="{TemplateBinding DropDownHeight}"
										MaxWidth="{TemplateBinding DropDownMaxWidth}"
										MaxHeight="{TemplateBinding DropDownMaxHeight}">
                                    <ContentPresenter x:Name="DropDownPopupContent"
											Content="{TemplateBinding DropDownContent}"
											ContentTemplate="{TemplateBinding DropDownContentTemplate}"
											HorizontalAlignment="Stretch"
											VerticalAlignment="Stretch"
											DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}" />
                                </Border>
                            </Grid>
                        </Popup>

                        <!-- Focus -->
                        <Border x:Name="CommonStatesWrapper">
                            <Border x:Name="FocusVisual" Visibility="Collapsed" Opacity="0" 
                                    CornerRadius="{TemplateBinding CornerRadius}" 
                                    BorderThickness="1" BorderBrush="{StaticResource MarkerBrush}"  />
                        </Border>

                    </Grid>

                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="Border" Property="Background" Value="{StaticResource BasicBrush}" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="CommonStatesWrapper" Property="Opacity" Value="0" />
                            <Setter TargetName="Border" Property="Background" Value="{StaticResource BasicBrush}" />
                            <Setter TargetName="Content" Property="Margin" Value="0 0 0 -1" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Content" Property="Opacity" Value="0.5" />
                            <Setter TargetName="DropDownIndicator" Property="Opacity" Value="0.5" />
                        </Trigger>
                        <Trigger Property="IsOpen" Value="True">
                            <Setter TargetName="CheckedVisual" Property="Opacity" Value="1" />
                            <Setter Property="Fill" TargetName="DropDownIndicator" Value="{StaticResource MainBrush}" />
                            <Setter Property="Foreground" TargetName="Content" Value="{StaticResource MainBrush}" />
                        </Trigger>
                        <Trigger Property="DropDownButtonPosition" Value="Bottom" />
                        <Trigger Property="DropDownButtonPosition" Value="Left">
                            <Setter TargetName="DropDownIndicator" Property="Grid.Column" Value="0" />
                            <Setter TargetName="DropDownIndicator" Property="Grid.Row" Value="1" />
                        </Trigger>
                        <Trigger Property="DropDownButtonPosition" Value="Top">
                            <Setter TargetName="DropDownIndicator" Property="Grid.Column" Value="1" />
                            <Setter TargetName="DropDownIndicator" Property="Grid.Row" Value="0" />
                        </Trigger>
                        <Trigger Property="DropDownButtonPosition" Value="Right">
                            <Setter TargetName="DropDownIndicator" Property="Grid.Column" Value="2" />
                            <Setter TargetName="DropDownIndicator" Property="Grid.Row" Value="1" />
                        </Trigger>
                        <Trigger Property="DropDownPlacement" Value="Absolute" />
                        <Trigger Property="DropDownPlacement" Value="Bottom" />
                        <Trigger Property="DropDownPlacement" Value="Center" />
                        <Trigger Property="DropDownPlacement" Value="Top">
                            <Setter TargetName="DropDownPopup" Property="Placement" Value="Top" />
                            <Setter TargetName="DropDownIndicator" Property="LayoutTransform">
                                <Setter.Value>
                                    <TransformGroup>
                                        <RotateTransform Angle="180" />
                                    </TransformGroup>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                        <Trigger Property="DropDownPlacement" Value="Left">
                            <Setter TargetName="DropDownPopup" Property="Placement" Value="Left" />
                            <Setter TargetName="DropDownIndicator" Property="LayoutTransform">
                                <Setter.Value>
                                    <TransformGroup>
                                        <RotateTransform Angle="90" />
                                    </TransformGroup>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                        <Trigger Property="DropDownPlacement" Value="Right">
                            <Setter TargetName="DropDownPopup" Property="Placement" Value="Right" />
                            <Setter TargetName="DropDownIndicator" Property="LayoutTransform">
                                <Setter.Value>
                                    <TransformGroup>
                                        <RotateTransform Angle="-90" />
                                    </TransformGroup>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="ToolBarRadSplitButton" TargetType="telerik:RadSplitButton">
        <Setter Property="Margin" Value="2" />
        <Setter Property="IsTabStop" Value="false" />
        <Setter Property="IsOpen" Value="false" />
        <Setter Property="Padding" Value="2" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="FontFamily" Value="Segoe UI" />
    </Style>

    <ControlTemplate TargetType="ToggleButton" x:Key="ToolBarDropdownButtonH">
        <Grid Background="Transparent">
            

            <Border x:Name="Border" Background="Transparent" />

            <Path x:Name="Path" Opacity="1" Data="M0,3.3000627 L4,3.3000627 L4,7.3000627 z M3.3000572,0 L7.3000574,0 L7.3000574,4 z" Fill="{StaticResource StrongBrush}" 
                  HorizontalAlignment="Center" Margin="0 4 0 0" RenderTransformOrigin="0.8,0.2" 
                  Stretch="Fill"  Width="7.3" Height="7.3" VerticalAlignment="Center">
                <Path.RenderTransform>
                    <TransformGroup>
                        <RotateTransform Angle="45" />
                        <TranslateTransform X="0.535166" Y="-0.379606" />
                    </TransformGroup>
                </Path.RenderTransform>
            </Path>
            
            <Path x:Name="PathCheck" Opacity="0" Data="M0.70710689,0.93929797 L4.242641,4.4748321 L3.5355341,5.1819391 L0,1.6464047 z M1.1820216,0 L5.1820216,0 L5.1820216,4 z" 
                  Fill="{StaticResource StrongBrush}" RenderTransformOrigin="0.6,0.4" Stretch="Fill" 
                  Margin="0 0 0 4" Height="5.182" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="5.182">
                <Path.RenderTransform>
                    <RotateTransform Angle="135" />
                </Path.RenderTransform>
            </Path>

        </Grid>
        
        <ControlTemplate.Triggers>
            <Trigger Property="IsChecked" Value="True">
                <Setter TargetName="Path" Property="Opacity" Value="0" />
                <Setter TargetName="PathCheck" Property="Opacity" Value="1" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter TargetName="Border" Property="Background" Value="{StaticResource BasicBrush}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter TargetName="Border" Property="Background" Value="{StaticResource BasicBrush}" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Path" Property="Opacity" Value="0.5" />
                <Setter TargetName="PathCheck" Property="Opacity" Value="0.5" />
            </Trigger>
        </ControlTemplate.Triggers>
        
    </ControlTemplate>

    <Style TargetType="telerikNavigation:RadToolBar">
        <Setter Property="FontFamily" Value="Segoe UI" />
        <Setter Property="BorderBrush" Value="{StaticResource BasicBrush}" />
        <Setter Property="Background" Value="{StaticResource MainBrush}" />
        <Setter Property="BorderThickness" Value="1" />

        
        <Setter Property="FocusManager.IsFocusScope"
                Value="True" />
        
        <Setter Property="ItemContainerStyleSelector">
            <Setter.Value>
                <telerikNavigation:ToolBarContainerStyleSelector>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadToolBarSeparator"
                                                             ContainerStyle="{StaticResource ToolBarSeparator}" />
                    <telerikNavigation:ToolBarContainerStyle TypeName="TextBlock"
                                                             ContainerStyle="{StaticResource ToolBarTextBlock}" />
                    <telerikNavigation:ToolBarContainerStyle TypeName="TextBox"
                                                             ContainerStyle="{StaticResource  {telerik:ThemeResourceKey ThemeType=telerik:MetroTheme , ElementType=TextBox}}" />
                    <telerikNavigation:ToolBarContainerStyle TypeName="Button"
                                                             ContainerStyle="{StaticResource ToolBarButton}" />
                    <telerikNavigation:ToolBarContainerStyle TypeName="ToggleButton"
                                                             ContainerStyle="{StaticResource ToolBarToggleButton}" />
                    <telerikNavigation:ToolBarContainerStyle TypeName="CheckBox"
                                                             ContainerStyle="{StaticResource  {telerik:ThemeResourceKey ThemeType=telerik:MetroTheme , ElementType=CheckBox}}" />
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadioButton"
                                                             ContainerStyle="{StaticResource  {telerik:ThemeResourceKey ThemeType=telerik:MetroTheme , ElementType=RadioButton}}" />
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadButton"
                                                             ContainerStyle="{StaticResource ToolBarRadButton}" />
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadToggleButton"
                                                             ContainerStyle="{StaticResource ToolBarRadToggleButton}" />
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadRadioButton"
                                                             ContainerStyle="{StaticResource ToolBarRadRadioButton}" />
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadDropDownButton"
                                                             ContainerStyle="{StaticResource ToolBarRadDropDownButton}" />
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadSplitButton"
                                                             ContainerStyle="{StaticResource ToolBarRadSplitButton}" />
                </telerikNavigation:ToolBarContainerStyleSelector>
            </Setter.Value>
        </Setter>
        <Setter Property="MinWidth" Value="25" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadToolBar">
                    <Grid MinHeight="28" x:Name="RootElement" SnapsToDevicePixels="True"
                         >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition x:Name="c0" Width="Auto" />
                            <ColumnDefinition x:Name="c1" Width="*" />
                            <ColumnDefinition x:Name="c2" Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition x:Name="r0" Height="Auto" />
                            <RowDefinition x:Name="r1" Height="*" />
                            <RowDefinition x:Name="r2" Height="Auto"/>
                        </Grid.RowDefinitions>
                        

                        <Border x:Name="InnerBackground" Grid.ColumnSpan="3" Grid.RowSpan="3"
								BorderBrush="{TemplateBinding BorderBrush}" 
								HorizontalAlignment="Stretch" 
								BorderThickness="{TemplateBinding BorderThickness}"
								Background="{TemplateBinding Background}" />
                        
                        
                                <ToggleButton Visibility="{TemplateBinding OverflowButtonVisibility}"
                                        x:Name="PART_DropdownButton" Template="{StaticResource ToolBarDropdownButtonH}"
                                        IsEnabled="{TemplateBinding HasOverflowItems}"
                                        IsChecked="{TemplateBinding IsOverflowOpen}"
                                        Margin="-2 0 0 0"
                                    Grid.Column="2" Grid.RowSpan="2" Width="12"/>
                                    
                                    
                            
                        
                        <Grid Grid.Column="2">
                            <Popup x:Name="PART_Overflow" AllowsTransparency="True"
									StaysOpen="False" Focusable="false" Placement="Bottom"
									IsOpen="{Binding Path=IsOverflowOpen, RelativeSource={RelativeSource TemplatedParent}}"
									PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}">
                                
                                
                                    <Border Margin="0" MinWidth="55" MinHeight="33">
                                        <Border
												BorderBrush="{StaticResource BasicBrush}"
												Background="{StaticResource MainBrush}"
												BorderThickness="1" Padding="1,2,1,2">
                                            
                                            <telerikToolBar:RadToolBarPanel 
                                                
                                                    x:Name="PART_OverflowPanel"
											Orientation="Vertical" VerticalAlignment="Top" />
                                        </Border>
                                    </Border>
                                </Popup>
                                
                        </Grid>
                        
                        

                        <Grid Visibility="{TemplateBinding GripVisibility}"  Grid.RowSpan="2">
                            <Grid x:Name="GripOrnamentH" Visibility="Visible">
                                <Path Fill="{StaticResource StrongBrush}" Stretch="Fill" Margin="5,1,0,0" Width="2"
									Height="14" VerticalAlignment="Center"
									HorizontalAlignment="Left"
									Data="M0,12 L2,12 2,14 0,14 0,12 z M0,8 L2,8 2,10 0,10 0,8 z M0,4 L2,4 2,6 0,6 0,4 z M0,0 L2,0 2,2 0,2 0,0 z" />
                            </Grid>
                            <Grid x:Name="GripOrnamentV" Visibility="Collapsed">
                                <Path Fill="{StaticResource StrongBrush}" Stretch="Fill" Margin="1,5,0,0" Width="14"
									Height="2" VerticalAlignment="Top"
									HorizontalAlignment="Center"
									Data="M12,0 L12,2 14,2 14,0 12,0 z M8,0 L8,2 10,2 10,0 8,0 z M4,0 L4,2 6,2 6,0 4,0 z M0,0 L0,2 2,2 2,0 0,0 z" />
                            </Grid>
                        </Grid>
                        
                        <telerikToolBar:RadToolBarPanel 
                        
                        
                            x:Name="PART_StripPanel" Grid.Column="1" Margin="0,1,4,1"
							Orientation="Horizontal" />
                    </Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter TargetName="RootElement" Property="Opacity" Value="0.5" />
                        </Trigger>
                        <Trigger Property="Orientation" Value="Vertical">
                            <Setter TargetName="c0" Property="Width" Value="*" />
                            <Setter TargetName="c1" Property="Width" Value="Auto" />
                            <Setter TargetName="c2" Property="Width" Value="Auto" />

                            <Setter TargetName="PART_StripPanel" Property="Orientation" Value="Vertical" />
                            <Setter TargetName="PART_StripPanel" Property="Grid.Row" Value="1" />
                            <Setter TargetName="PART_StripPanel" Property="Grid.Column" Value="0" />
                            <Setter TargetName="PART_StripPanel" Property="Margin" Value="1,0,1,4" />

                            <Setter TargetName="PART_DropdownButton" Property="Grid.Row" Value="2" />
                            <Setter TargetName="PART_DropdownButton" Property="Grid.Column" Value="0" />
                            <Setter TargetName="PART_DropdownButton" Property="Margin" Value="0 -2 0 0" />
                            <Setter TargetName="PART_DropdownButton" Property="LayoutTransform">
                                <Setter.Value>
                                    <TransformGroup>
                                        <RotateTransform Angle="-90" CenterX="0" CenterY="0" />
                                        <ScaleTransform ScaleX="1" ScaleY="-1" />
                                    </TransformGroup>
                                </Setter.Value>
                            </Setter>

                            <Setter TargetName="GripOrnamentH" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="GripOrnamentV" Property="Visibility" Value="Visible" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="telerikNavigation:RadToolBarTray">
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikToolBar:RadToolBarTrayPanel x:Name="PART_BandPanel" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadToolBarTray">
                    <Border Padding="{TemplateBinding Padding}"
							Background="{TemplateBinding Background}"
							BorderBrush="{TemplateBinding BorderBrush}"
							BorderThickness="{TemplateBinding BorderThickness}">
                        <ItemsPresenter x:Name="PART_ItemsPresenter" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    

</ResourceDictionary>
