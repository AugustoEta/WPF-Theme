    <Style x:Key="ApplicationButtonStyleOffice2010" TargetType="telerikRibbonBar:RadRibbonDropDownButton">
        <Setter Property="Padding" Value="0" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Margin" Value="2,0,0,0" />
        <Setter Property="Foreground" Value="#FFFFFFFF" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="DropDownIndicatorVisibility" Value="Collapsed" />
        <Setter Property="IsAutoSize" Value="False" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonDropDownButton">
                    <Grid Width="46" Height="22" Background="Transparent">
                        

                        <Grid x:Name="ContentGrid" Margin="{TemplateBinding Padding}">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <ContentPresenter Grid.ColumnSpan="3" Grid.RowSpan="2" Content="{TemplateBinding Content}"
									HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
									VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
									ContentTemplate="{TemplateBinding ContentTemplate}" />

                            <Image x:Name="Image" Stretch="None" Grid.ColumnSpan="3" Grid.RowSpan="2"
									HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
									VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />

                            <TextBlock x:Name="textRow1" Grid.Row="1" Margin="0,2,0,0" Text="{TemplateBinding TextRow1}"
									TextAlignment="Center" />

                            <StackPanel x:Name="DropDownPanel" Grid.Row="1" Margin="0,14,0,0"
									HorizontalAlignment="Center" Orientation="Horizontal" MinHeight="14" MaxHeight="14"
									Height="14">
                                <TextBlock x:Name="textRow2" Text="{TemplateBinding TextRow2}" />
                            </StackPanel>

                            <Path x:Name="DropDownIndicator"
								 Grid.Row="2" Grid.Column="1"
								 IsHitTestVisible="False"
								 Visibility="{TemplateBinding DropDownIndicatorVisibility}"
								 Fill="{TemplateBinding Foreground}"
								 HorizontalAlignment="Center"
								 VerticalAlignment="Center"
								 RenderTransformOrigin="0.5,0.5"
								 Margin="5,3,5,3"
								 Data="M0,0 L5,0 5,1 4,1 4,2 3,2 3,3 2,3 2,2 1,2 1,1 0,1 0,0 z"
								 SnapsToDevicePixels="True"
                                 />
                        </Grid>

                        
                        <Popup x:Name="DropDownPopup" HorizontalOffset="0" VerticalOffset="-22"
								AllowsTransparency="True" StaysOpen="False" Focusable="false" Placement="Bottom"
								IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
                            
                            
                                <!--PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}">-->
                                <Border x:Name="DropDownPopupBorder" BorderBrush="Transparent" Background="Transparent"
										BorderThickness="0" Padding="0" MinWidth="33" MinHeight="33">
                                    <ContentControl x:Name="DropDownPopupContent"
											Content="{TemplateBinding DropDownContent}" HorizontalAlignment="Stretch"
											VerticalAlignment="Stretch" />
                                </Border>
                                
                        </Popup>
                        
                        

                        <Rectangle x:Name="DisabledIndicator" Visibility="Collapsed" Fill="#99FFFFFF" />
                    </Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="ContentGrid" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="ContentGrid" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False" />

                        <Trigger Property="IsOpen" Value="True">
                            <Setter TargetName="ContentGrid" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="DropDownButtonPosition" Value="Top" />
                        <Trigger Property="DropDownButtonPosition" Value="Left" />
                        <Trigger Property="DropDownButtonPosition" Value="Bottom" />
                        <Trigger Property="DropDownButtonPosition" Value="Right">
                            <Setter TargetName="DropDownPanel" Property="Grid.Column" Value="2" />
                            <Setter TargetName="DropDownPanel" Property="Grid.Row" Value="0" />
                            <Setter TargetName="DropDownPanel" Property="Margin" Value="0,0,0,0" />
                            <Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="CurrentSize" Value="Large" />
                        <Trigger Property="CurrentSize" Value="Medium">
                            <Setter TargetName="textRow1" Property="Grid.Row" Value="0" />
                            <Setter TargetName="textRow1" Property="Grid.Column" Value="0" />
                            <Setter TargetName="textRow1" Property="Margin" Value="4,0,1,0" />
                            <Setter TargetName="textRow1" Property="VerticalAlignment" Value="Center" />
                            <Setter TargetName="textRow1" Property="TextAlignment" Value="Left" />
                            <Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="CurrentSize" Value="Small">
                            <Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="DropDownPlacement" Value="Absolute" />
                        <Trigger Property="DropDownPlacement" Value="Bottom" />
                        <Trigger Property="DropDownPlacement" Value="Center" />
                        <Trigger Property="DropDownPlacement" Value="Top" />

                        <Trigger Property="DropDownPlacement" Value="Left" />

                        <Trigger Property="DropDownPlacement" Value="Right" />

                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- ApplicationButtonStyle -->
    <Style x:Key="ApplicationButtonStyleOffice2007" TargetType="telerikRibbonBar:RadRibbonDropDownButton">
        <Setter Property="Padding" Value="0" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Margin" Value="4,4,0,5" />
        
        <Setter Property="FocusVisualStyle" Value="{StaticResource ApplicationButtonFocusVisual}" />
        
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonDropDownButton">
                    <Grid Width="37" Height="37">
                        

                        <Ellipse x:Name="Ellipse" Stroke="#FFFFFFFF" StrokeThickness="2" Opacity="0.7"/>
                        
                        <Grid Margin="{TemplateBinding Padding}"
								HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
								VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" MaxHeight="28" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <ContentPresenter Grid.ColumnSpan="3" Grid.RowSpan="2"
									HorizontalAlignment="Center" VerticalAlignment="Center"
									Content="{TemplateBinding Content}"
									ContentTemplate="{TemplateBinding ContentTemplate}" />

                            <Image x:Name="Image" Stretch="None" Grid.ColumnSpan="3"
									Grid.RowSpan="2" />

                            <TextBlock x:Name="textRow1" Grid.Row="1" Margin="0,2,0,0"
									Text="{TemplateBinding TextRow1}" TextAlignment="Center" />

                            <StackPanel x:Name="DropDownPanel" Grid.Row="1" Margin="0,14,0,0"
									HorizontalAlignment="Center" Orientation="Horizontal"
									MinHeight="14" MaxHeight="14" Height="14">
                                <TextBlock x:Name="textRow2" Text="{TemplateBinding TextRow2}" />
                            </StackPanel>
                        </Grid>

                        <Popup x:Name="DropDownPopup"
							
                            AllowsTransparency="True" StaysOpen="False"
							Focusable="false" Placement="Bottom"
							IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}"
							HorizontalOffset="-5" VerticalOffset="-15"
                            
                            >
                            <!--PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}">-->
                            <Border x:Name="DropDownPopupBorder" BorderBrush="Transparent"
									Background="Transparent" BorderThickness="0" CornerRadius="1"
									Padding="0" 
                                
                                
                                MinWidth="33" MinHeight="33">
                                <ContentControl x:Name="DropDownPopupContent"
										Content="{TemplateBinding DropDownContent}"
										HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                            </Border>
                        </Popup>

                        <Rectangle x:Name="DisabledIndicator" Visibility="Collapsed"
								Fill="#99FFFFFF" />
                    </Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true" >
                            <Setter Value="1" TargetName="Ellipse" Property="Opacity" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True" />
                        <Trigger Property="IsEnabled" Value="False" />
                        <Trigger Property="IsOpen" Value="True" />
                        <Trigger Property="IsBackstageOpen" Value="True" />
                        <Trigger Property="DropDownButtonPosition" Value="Top" />
                        <Trigger Property="DropDownButtonPosition" Value="Left" />
                        <Trigger Property="DropDownButtonPosition" Value="Bottom" />
                        <Trigger Property="DropDownButtonPosition" Value="Right">
                            <Setter TargetName="DropDownPanel" Property="Grid.Column" Value="2" />
                            <Setter TargetName="DropDownPanel" Property="Grid.Row" Value="0" />
                            <Setter TargetName="DropDownPanel" Property="Margin" Value="0,0,0,0" />
                            <Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="CurrentSize" Value="Large" />
                        <Trigger Property="CurrentSize" Value="Medium">
                            <Setter TargetName="textRow1" Property="Grid.Row" Value="0" />
                            <Setter TargetName="textRow1" Property="Grid.Column" Value="0" />
                            <Setter TargetName="textRow1" Property="Margin" Value="4,0,1,0" />
                            <Setter TargetName="textRow1" Property="VerticalAlignment"
									Value="Center" />
                            <Setter TargetName="textRow1" Property="TextAlignment" Value="Left" />
                            <Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="CurrentSize" Value="Small">
                            <Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        
                        <Trigger Property="DropDownPlacement" Value="Absolute" />
                        <Trigger Property="DropDownPlacement" Value="Bottom" />
                        <Trigger Property="DropDownPlacement" Value="Center" />
                        <Trigger Property="DropDownPlacement" Value="Top" />
                        
                        <Trigger Property="DropDownPlacement" Value="Left" />
                        
                        <Trigger Property="DropDownPlacement" Value="Right" />
                        
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <!--  MinimizeButton -->
    <Style x:Key="MinimizeButtonStyle" TargetType="telerikRibbonBar:RadRibbonToggleButton">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Margin" Value="1" />
        <Setter Property="Padding" Value="2" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Size" Value="Small" />
        <Setter Property="Height" Value="21" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonToggleButton">
                    <Grid Width="20" Background="Transparent">
                        

                        <Border x:Name="OuterBorder" 
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" />

                        <Path x:Name="arrow" Fill="#FFFFFFFF" Opacity="0.7"  Stretch="None"  VerticalAlignment="Center"
								HorizontalAlignment="Center"
								Data="M0.25,4 L3.75,0.25 L4.75,0.25 L8.25,3.875 L8.3125,5.5 L7.25,6.627 L6.25,6.627 L4.25,4.627 L2.25,6.627 L1.25,6.627 L0.25,5.627 z"
								StrokeThickness="0.5" RenderTransformOrigin="0.5 0.5">
                            <Path.RenderTransform>
                                <RotateTransform x:Name="directionRotation" Angle="0" />
                            </Path.RenderTransform>
                        </Path>
                    </Grid>

                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="arrow" Property="Opacity" Value="1"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True" />
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Opacity" TargetName="arrow" Value="0.5" />
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="arrow" Property="LayoutTransform">
                                <Setter.Value>
                                    <TransformGroup>
                                        <RotateTransform Angle="180" />
                                    </TransformGroup>
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="arrow" Property="Opacity" Value="1"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                    

                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- TabItem  Style -->
    <Style TargetType="telerikNavigation:RadTabItem" x:Key="RibbonRadTabItemStyle">
        <Setter Property="Padding" Value="0,1,0,1" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="1,0,0,0" />
        <Setter Property="BorderBrush" Value="#FFFFFFFF" />
        <Setter Property="Foreground" Value="#FFFFFFFF" />
        <Setter Property="IsTabStop" Value="False" />
        
        <Setter Property="FocusVisualStyle">
            <Setter.Value>
                <Style TargetType="Control">
                    <Setter Property="Visibility" Value="Collapsed" />
                </Style>
            </Setter.Value>
        </Setter>
        
        <Setter Property="TopTemplate">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadTabItem">
                    <Grid>
                        
                        <Rectangle Margin="0 1 0 4" HorizontalAlignment="Left"
								Fill="{TemplateBinding BorderBrush}" VerticalAlignment="Stretch"
								Width="1" />
                        <Grid x:Name="wrapper" Margin="4,1,3,0" 
                               >

                            <Border Background="{TemplateBinding Background}" />

                            <Border x:Name="MouseOverVisual" Visibility="Collapsed" Opacity="0.7"  BorderThickness="1 1 1 0"  
                                    BorderBrush="#FFFFFFFF" />


                            <Border x:Name="SelectedVisual" Visibility="Collapsed" BorderThickness="1 1 1 0"
                                    Background="#FFFFFFFF" BorderBrush="#FFFFFFFF" />

                            <telerikNavigationPrimitives:TabItemContentPresenter Grid.Column="1"
									x:Name="HeaderElement" Content="{TemplateBinding Header}"
									ContentTemplate="{TemplateBinding HeaderTemplate}"
									VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
									HorizontalAlignment="Center" HorizontalContentAlignment="Center"
									VerticalAlignment="Center" Margin="0,3,0,4" Foreground="{TemplateBinding Foreground}" />
                           
                        </Grid>
                    </Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="true" />
                            
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter Property="Visibility" TargetName="MouseOverVisual" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter Property="Visibility" TargetName="SelectedVisual" Value="Visible" />
                            <Setter Property="Foreground" TargetName="HeaderElement" Value="{StaticResource StrongBrush}" />
                        </Trigger>

                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Opacity" Value="0.5" TargetName="HeaderElement" />
                        </Trigger>

                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- WindowTitle -->
    <Style x:Key="WindowTitleStyle" TargetType="telerikRibbonBarPrimitives:WindowTitle">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="FontSize" Value="11" />
        <Setter Property="IsHitTestVisible" Value="false" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="IsActive" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBarPrimitives:WindowTitle">
                    <StackPanel Orientation="Horizontal">
                        <TextBlock x:Name="Title" Text="{TemplateBinding Title}"
                                Foreground="#FFFFFFFF" />
                        <TextBlock x:Name="Divider" Text=" - " Visibility="{TemplateBinding DividerVisibility}"
                                telerik:LocalizationManager.ResourceKey="RibbonBarWindowTitleDivider"
                                Foreground="#FFFFFFFF" />
                        <TextBlock x:Name="ApplicationName" Text="{TemplateBinding ApplicationName}"
                                Foreground="#FFFFFFFF" />
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
