<ResourceDictionary 
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	
	xmlns:telerikChromes="clr-namespace:Telerik.Windows.Controls.Chromes;assembly=Telerik.Windows.Controls"

	
	xmlns:theme="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero"
	xmlns:themes="http://schemas.telerik.com/2008/xaml/presentation"
    xmlns:telerikRibbonBar="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.RibbonBar"
	xmlns:telerikRibbonBarPrimitives="clr-namespace:Telerik.Windows.Controls.RibbonBar.Primitives;assembly=Telerik.Windows.Controls.RibbonBar"
    xmlns:telerikNavigation="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Navigation"
    xmlns:telerikTabControl="clr-namespace:Telerik.Windows.Controls.TabControl;assembly=Telerik.Windows.Controls.Navigation"
	xmlns:telerikInput="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Input"
    xmlns:telerikNavigationPrimitives="clr-namespace:Telerik.Windows.Controls.Primitives;assembly=Telerik.Windows.Controls.Navigation"
    xmlns:telerikAnimation="clr-namespace:Telerik.Windows.Controls.Animation;assembly=Telerik.Windows.Controls"
    xmlns:telerikPrimitives="clr-namespace:Telerik.Windows.Controls.Primitives;assembly=Telerik.Windows.Controls"
	xmlns:telerik="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls"
    xmlns:telerikKeyTips="clr-namespace:Telerik.Windows.Controls.RibbonBar.KeyTips;assembly=Telerik.Windows.Controls.RibbonBar"
>

<telerik:MetroColors x:Key="MetroColors" />
<SolidColorBrush x:Key="BasicBrush" Color="{Binding Source={StaticResource MetroColors}, Path=Palette.BasicColor}" />
<SolidColorBrush x:Key="AccentBrush" Color="{Binding Source={StaticResource MetroColors}, Path=Palette.AccentColor}" />
<SolidColorBrush x:Key="StrongBrush" Color="{Binding Source={StaticResource MetroColors}, Path=Palette.StrongColor}" />
<telerik:MetroTheme x:Key="Theme" />
<ImageBrush ImageSource="/Telerik.Windows.Controls.RibbonBar;component/RibbonGroupDialogLauncherVista.png" x:Key="RibbonGroupDialogLauncher" Stretch="None" />
<SolidColorBrush x:Key="MainBrush" Color="{Binding Source={StaticResource MetroColors}, Path=Palette.MainColor}" />


    
    <Style x:Key="ApplicationRibbonButton" TargetType="telerikRibbonBar:RadRibbonButton">
        <Setter Property="Padding" Value="5" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Size" Value="Large" />
        <!--<Setter Property="SplitText" Value="False" />-->
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonButton">
                    <Grid>
                        
                        <Border x:Name="buttonSurface" BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}"
								/>

                        <Grid x:Name="ContentGrid" Margin="{TemplateBinding Padding}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>

                            <ContentPresenter Content="" Grid.ColumnSpan="2" />

                            <Image Name="Image" />

                            <TextBlock Name="textRow1" Grid.Column="1" Margin="4,0,1,0" Text="{TemplateBinding Text}"
									TextAlignment="Left" VerticalAlignment="Center" />
                        </Grid>
                    </Grid>

                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter Property="Background" TargetName="buttonSurface"
									Value="{StaticResource BasicBrush}" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Background" TargetName="buttonSurface"
									Value="{StaticResource BasicBrush}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="ContentGrid" Property="Opacity" Value="1" />
                        </Trigger>

                    </ControlTemplate.Triggers>
                    

                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="ApplicationRibbonDropDownButton" TargetType="telerikRibbonBar:RadRibbonDropDownButton">
        <Setter Property="Padding" Value="5" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Size" Value="Large" />
        <!--<Setter Property="SplitText" Value="False" />-->
        <Setter Property="AutoOpenDelay" Value="0:0:0.3" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonDropDownButton">
                    <Grid SnapsToDevicePixels="True"
                         >
                        
                        <Border x:Name="buttonSurface" BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" />

                        <Border x:Name="isOpenIndicator" Visibility="Collapsed"
								Background="{StaticResource AccentBrush}" />


                        <Grid Margin="{TemplateBinding Padding}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="22" />
                            </Grid.ColumnDefinitions>

                            <ContentControl Grid.ColumnSpan="3" x:Name="ContentPresenter"
                                    Foreground="{TemplateBinding Foreground}"
									HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
									VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
									Content="{TemplateBinding Content}"
									ContentTemplate="{TemplateBinding ContentTemplate}" />

                            <Image Name="Image" />

                            <TextBlock Name="textRow1" Grid.Column="1" Margin="4,0,1,0" Text="{TemplateBinding Text}"
									TextAlignment="Left" VerticalAlignment="Center" />

                            <Border Name="DropDownPanel" Grid.Column="2" VerticalAlignment="Stretch">
                                <Path Name="DropDownIndicator" VerticalAlignment="Center" HorizontalAlignment="Center"
										Data="M 0,0 L 3.5,3.5 L 0,7" Margin="2,0,0,0"
										Visibility="{TemplateBinding DropDownIndicatorVisibility}"
										Fill="{StaticResource StrongBrush}" />
                            </Border>
                        </Grid>
                        
                        <Popup x:Name="DropDownPopup" HorizontalOffset="2" VerticalOffset="-1" AllowsTransparency="True"
								StaysOpen="False" Focusable="false" Placement="Bottom"
								IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
                            
                            
                                <Grid>

                                    <Border x:Name="DropDownPopupBorder"
											BorderBrush="{StaticResource BasicBrush}"
											Background="#FFFFFFFF"
											BorderThickness="1" MinWidth="3" Width="{TemplateBinding DropDownWidth}"
											MinHeight="3" Height="{TemplateBinding DropDownHeight}">
                                        <ContentControl x:Name="DropDownPopupContent"
												Content="{TemplateBinding DropDownContent}"
												HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                                    </Border>
                                </Grid>
                                
                        </Popup>
                        
                        

                        <Rectangle x:Name="DisabledIndicator" Visibility="Collapsed" Fill="#99FFFFFF" />
                    </Grid>

                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter Property="Background" TargetName="buttonSurface"
									Value="{StaticResource BasicBrush}" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Background" TargetName="buttonSurface"
									Value="{StaticResource BasicBrush}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="DisabledIndicator" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsOpen" Value="True">
                            <Setter TargetName="isOpenIndicator" Property="Visibility" Value="Visible" />
                            <Setter TargetName="DropDownIndicator" Property="Fill" Value="#FFFFFFFF" />
                            <Setter TargetName="ContentPresenter" Property="Foreground" Value="#FFFFFFFF" />
                            <Setter TargetName="textRow1" Property="Foreground" Value="#FFFFFFFF" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <ControlTemplate TargetType="telerik:RadButton" x:Key="AppMenuRadSplitButton_ButtonStyle">
        <Grid>
            

            <Border x:Name="backgroundDecorator" BorderThickness="{TemplateBinding BorderThickness}"
					BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}">
                <ContentPresenter x:Name="Content" Content="{TemplateBinding Content}"
							ContentTemplate="{TemplateBinding ContentTemplate}"
							VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
							HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
							Margin="{TemplateBinding Padding}"
							RecognizesAccessKey="True"
                     />
            </Border>
        </Grid>

        
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False" />
            <Trigger Property="IsDefault" Value="True" />
            <Trigger Property="IsMouseOver" Value="True">

            </Trigger>
            <Trigger Property="IsPressed" Value="True">

            </Trigger>
        </ControlTemplate.Triggers>
        
    </ControlTemplate>

    <ControlTemplate TargetType="telerik:RadToggleButton" x:Key="AppMenuRadSplitButton_DropDownStyle">
        <Grid>
            

            <Border x:Name="backgroundDecorator" BorderThickness="{TemplateBinding BorderThickness}"
					BorderBrush="{TemplateBinding BorderBrush}" 
                    Background="{TemplateBinding Background}" />

            <Border Opacity="0" x:Name="CheckDecorator" 
                    Background="{StaticResource AccentBrush}" />

            <Border BorderThickness="{TemplateBinding BorderThickness}">
                <ContentControl  x:Name="ContentPresenter"
                        Foreground="{TemplateBinding Foreground}" 
                        Content="{TemplateBinding Content}"
						ContentTemplate="{TemplateBinding ContentTemplate}"
						VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
						HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
						Margin="{TemplateBinding Padding}"/>
            </Border>
        </Grid>

        
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False" />
            <Trigger Property="IsFocused" Value="True" />
            <Trigger Property="IsMouseOver" Value="True">
                
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
              
            </Trigger>
            <Trigger Property="IsChecked" Value="True">
               
                <Setter TargetName="ContentPresenter" Property="Foreground"  Value="#FFFFFFFF" />
            </Trigger>
        </ControlTemplate.Triggers>
        
    </ControlTemplate>

    <Style x:Key="ApplicationRibbonSplitButton" TargetType="telerikRibbonBar:RadRibbonSplitButton">
        <Setter Property="IsTabStop" Value="false" />
        <Setter Property="IsOpen" Value="false" />
        <Setter Property="Padding" Value="5" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        
        
        <Setter Property="DropDownPlacement" Value="Relative" />
        
        <Setter Property="Size" Value="Large" />
        <!--<Setter Property="SplitText" Value="False" />-->
        <Setter Property="AutoOpenDelay" Value="0:0:0.3" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonSplitButton">
                    <Grid SnapsToDevicePixels="True"
                         >
                        

                        <Border x:Name="backgroundDecorator" BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}" 
                                Background="{TemplateBinding Background}" />


                        <Border x:Name="mouseOverDecorator" Opacity="0" 
                                Background="{StaticResource BasicBrush}" />

                        <Border x:Name="CheckDecorator" Opacity="0" 
                                Background="{StaticResource AccentBrush}" />

                        <Grid Margin="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" MaxHeight="28" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <telerik:RadButton x:Name="ButtonPart" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2"
									BorderThickness="0" Background="Transparent" Content="{TemplateBinding Content}"
									ContentTemplate="{TemplateBinding ContentTemplate}"
									HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
									VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
									Template="{StaticResource AppMenuRadSplitButton_ButtonStyle}"
									Padding="{TemplateBinding Padding}" />
                            <telerik:RadToggleButton x:Name="DropDownPart" Grid.Row="0" Grid.Column="2"
									BorderThickness="0" Margin="0" Background="Transparent"
									Visibility="{TemplateBinding DropDownIndicatorVisibility}"
									Template="{StaticResource AppMenuRadSplitButton_DropDownStyle}" IsTabStop="false"
								
                                
                                IsChecked="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                 />
                                <Image Name="Image" IsHitTestVisible="False" Margin="4" />

                                <TextBlock Name="textRow1" Grid.Row="0" Grid.Column="1" Margin="0,0,2,0"
										IsHitTestVisible="False" Text="{TemplateBinding Text}" TextAlignment="Left"
										VerticalAlignment="Center" />

                                <Grid Name="DropDownPanel" Grid.Row="0" Grid.Column="2" Margin="0" Width="22"
										IsHitTestVisible="False">
                                    <Path Name="DropDownIndicator" VerticalAlignment="Center"
											HorizontalAlignment="Center" Data="M 0,0 L 3.5,3.5 L 0,7" Margin="0"
											IsHitTestVisible="False"
											Visibility="{TemplateBinding DropDownIndicatorVisibility}"
											Fill="{StaticResource StrongBrush}" />
                                </Grid>
                        </Grid>
                        
                        <Popup x:Name="DropDownPopup" HorizontalOffset="2" VerticalOffset="-1" AllowsTransparency="True"
								StaysOpen="False" Focusable="false" Placement="Bottom"
								IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
                            
                            
                                <Grid>
                                    <Border x:Name="DropDownPopupBorder"
											BorderBrush="{StaticResource BasicBrush}"
											Background="#FFFFFFFF"
											BorderThickness="1" MinWidth="3" Width="{TemplateBinding DropDownWidth}"
											MinHeight="3" Height="{TemplateBinding DropDownHeight}">
                                        <ContentControl x:Name="DropDownPopupContent"
												Content="{TemplateBinding DropDownContent}"
												HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                                    </Border>
                                </Grid>
                                
                        </Popup>
                        
                        

                        <Rectangle x:Name="disabledDecorator" Visibility="Collapsed" Opacity="0" Fill="#FFFFFFFF" />
                    </Grid>

                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="mouseOverDecorator" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="disabledDecorator" Property="Opacity" Value="0.55" />
                            <Setter TargetName="disabledDecorator" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsFocused" Value="True" />
                        <Trigger Property="IsOpen" Value="True">
                            <Setter TargetName="mouseOverDecorator" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="DropDownButtonPosition" Value="Top" />
                        <Trigger Property="DropDownButtonPosition" Value="Left" />
                        <Trigger Property="DropDownButtonPosition" Value="Bottom" />
                        <Trigger Property="DropDownButtonPosition" Value="Right" />
                        <Trigger Property="CurrentSize" Value="Large" />
                        <Trigger Property="CurrentSize" Value="Medium" />
                        <Trigger Property="CurrentSize" Value="Small" />
                        <Trigger Property="DropDownPlacement" Value="Top" />
                        <Trigger Property="DropDownPlacement" Value="Left" />
                        <Trigger Property="DropDownPlacement" Value="Right" />
                        <Trigger Property="DropDownPlacement" Value="Bottom" />
                        <Trigger Property="DropDownPlacement" Value="Center" />
                        <Trigger Property="DropDownPlacement" Value="Relative">
                            <Setter TargetName="DropDownPopup" Property="Placement" Value="Relative" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <Style TargetType="telerikRibbonBar:Separator" x:Key="ApplicationSeparatorStyle">
        <Setter Property="BorderThickness" Value="0,1,0,0" />
        <Setter Property="BorderBrush" Value="{StaticResource BasicBrush}" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Height" Value="1" />
        <Setter Property="Margin" Value="44,0,0,0" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:Separator">
                    <Border BorderThickness="{TemplateBinding BorderThickness}"
							BorderBrush="{TemplateBinding BorderBrush}" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="telerikRibbonBar:ApplicationMenu">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Background" Value="#FFFFFFFF" />
        <Setter Property="BorderBrush" Value="{StaticResource BasicBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Foreground" Value="{StaticResource StrongBrush}" />
        <Setter Property="ItemContainerStyleSelector">
            <Setter.Value>
                <telerikRibbonBar:CustomContainerStyleSelector>
                    <telerikRibbonBar:CustomContainerStyle TypeName="RadRibbonButton"
							ContainerStyle="{StaticResource ApplicationRibbonButton}" />
                    <telerikRibbonBar:CustomContainerStyle TypeName="RadRibbonDropDownButton"
							ContainerStyle="{StaticResource ApplicationRibbonDropDownButton}" />
                    <telerikRibbonBar:CustomContainerStyle TypeName="RadRibbonSplitButton"
							ContainerStyle="{StaticResource ApplicationRibbonSplitButton}" />
                    <telerikRibbonBar:CustomContainerStyle TypeName="Separator"
							ContainerStyle="{StaticResource ApplicationSeparatorStyle}" />
                </telerikRibbonBar:CustomContainerStyleSelector>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:ApplicationMenu">
                    <Grid x:Name="rootGrid" SnapsToDevicePixels="True"
                         >
                        <Border Name="PART_OuterBorder"  BorderThickness="1 0 1 1"
								BorderBrush="{TemplateBinding BorderBrush}" Background="Transparent">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Border x:Name="clippingRectangle"  Grid.Row="0" Height="21" Margin="45 0 0 0"
										Background="{StaticResource AccentBrush}"  />
                                
                                <Rectangle x:Name="buttonRectangle" Grid.Row="0" Height="21" Width="44"
										HorizontalAlignment="Left" Fill="Transparent" />
                                
                                <Border Grid.Row="1" Background="#FFFFFFFF">

                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Border Name="LeftPane" Background="{TemplateBinding Background}" >
                                            <ItemsPresenter />
                                        </Border>
                                        <Border Name="RightPane"
												Visibility="{TemplateBinding RightPaneVisibility}"
												Grid.Column="1"
												BorderBrush="{StaticResource BasicBrush}"
												Background="#FFFFFFFF"
												BorderThickness="1,0,0,0">
                                            <ContentPresenter Content="{TemplateBinding Content}"
													Name="ContentPresenter" MinWidth="100" />
                                        </Border>
                                    </Grid>

                                </Border>
                                <Border Grid.Row="2" BorderThickness="0,1,0,0" BorderBrush="{StaticResource BasicBrush}"
                                        Background="#FFFFFFFF">
                                    <ContentPresenter Content="{TemplateBinding FooterContent}"
											HorizontalAlignment="Right" VerticalAlignment="Center" />
                                </Border>
                            </Grid>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    

	<Style TargetType="telerikRibbonBarPrimitives:DropShadow">
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="HorizontalContentAlignment" Value="Stretch" />
		<Setter Property="VerticalContentAlignment" Value="Stretch" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBarPrimitives:DropShadow">
					<Grid>
						<!-- Shadow -->
						<Rectangle Name="Left" HorizontalAlignment="Left" VerticalAlignment="Stretch" IsHitTestVisible="False" />
						<Rectangle Name="TopLeft" HorizontalAlignment="Left" VerticalAlignment="Top"  IsHitTestVisible="False" />
						<Rectangle Name="Top" HorizontalAlignment="Stretch" VerticalAlignment="Top" IsHitTestVisible="False" />
						<Rectangle Name="TopRight" HorizontalAlignment="Right" VerticalAlignment="Top" IsHitTestVisible="False" />
						<Rectangle Name="Right" HorizontalAlignment="Right" VerticalAlignment="Stretch" IsHitTestVisible="False" />
						<Rectangle Name="BottomRight" HorizontalAlignment="Right" VerticalAlignment="Bottom" IsHitTestVisible="False" />
						<Rectangle Name="Bottom" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" IsHitTestVisible="False" />
						<Rectangle Name="BottomLeft" HorizontalAlignment="Left" VerticalAlignment="Bottom" IsHitTestVisible="False" />

						<ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
    

    <Style TargetType="telerikRibbonBarPrimitives:GroupChrome">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Foreground" Value="{StaticResource StrongBrush}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBarPrimitives:GroupChrome">
                    <Grid SnapsToDevicePixels="True"
                         >
                        
                        <Border Name="OuterBorder" Background="#FFFFFFFF" BorderBrush="{StaticResource BasicBrush}" Margin="0,0,1,1" BorderThickness="0 0 1 0" />

                        <Border Name="Tray" Height="15" VerticalAlignment="Bottom" Margin="1,0,2,2">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <ContentControl Content="{TemplateBinding Header}"  ContentTemplate="{TemplateBinding HeaderTemplate}" Margin="3,0,3,0" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{StaticResource StrongBrush}" IsTabStop="False"/>

                                <telerikRibbonBar:RadRibbonButton telerik:StyleManager.Theme="{StaticResource Theme}" Visibility="{TemplateBinding DialogLauncherVisibility}" Name="DialogLauncher" Grid.Column="1" IsAutoSize="False" VerticalAlignment="Top" Padding="4,3,3,3" TabIndex="1" >
                                    <telerikRibbonBar:RadRibbonButton.Content>
                                        <Rectangle Fill="{StaticResource RibbonGroupDialogLauncher}" Width="8" Height="8" />
                                    </telerikRibbonBar:RadRibbonButton.Content>
                                </telerikRibbonBar:RadRibbonButton>
                            </Grid>
                        </Border>
                        <ContentPresenter Margin="4,2,4,18" />
                    </Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsCollapsed" Value="True">
                            <Setter TargetName="Tray" Property="Visibility" Value="Collapsed" />
                        </Trigger>

                        <Trigger Property="IsMouseOver" Value="True" >

                        </Trigger>

                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsMouseOver" Value="False" />
                                <Condition Property="IsPushed" Value="True" />
                                <Condition Property="IsCollapsed" Value="True" />
                            </MultiTrigger.Conditions>


                        </MultiTrigger>

                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsMouseOver" Value="False" />
                                <Condition Property="IsPushed" Value="False" />
                                <Condition Property="IsCollapsed" Value="False" />
                            </MultiTrigger.Conditions>

                        </MultiTrigger>

                    </ControlTemplate.Triggers>
                    

                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    
    

    <Style TargetType="telerikRibbonBar:RadButtonGroup">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Background" Value="#FFFFFFFF" />
        <Setter Property="BorderBrush" Value="{StaticResource BasicBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadButtonGroup">
                    <Border  BorderThickness="{TemplateBinding BorderThickness}" 
							 Background="{TemplateBinding Background}" 
							 BorderBrush="{TemplateBinding BorderBrush}" 
							 SnapsToDevicePixels="True"
                         >
                        <Grid>
                            <telerikRibbonBarPrimitives:ButtonGroupPanel Name="Panel" DividerDarkBrush="{StaticResource BasicBrush}" />
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    
    

    <Style TargetType="telerikRibbonBar:RadGallery">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="ItemWidth" Value="156"/>
        <Setter Property="ItemHeight" Value="26"/>
        <Setter Property="ViewportHeight" Value="184"/>
        <Setter Property="ViewportWidth" Value="157"/>        

        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                     <telerik:RadWrapPanel 
                        
                        
                        ItemWidth="{Binding RelativeSource={RelativeSource
						FindAncestor, AncestorType={x:Type telerikRibbonBar:RadRibbonGallery}},
						Path=ItemWidth}" ItemHeight="{Binding RelativeSource={RelativeSource
						FindAncestor, AncestorType={x:Type telerikRibbonBar:RadRibbonGallery}},
						Path=ItemHeight}"
                         />
                    
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadGallery">
                    <Grid >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <ScrollViewer x:Name="ScrollViewer"  
									  Grid.Row="0" IsTabStop="False" 
									  BorderThickness="0" Margin="1" 
									  Height="{TemplateBinding ViewportHeight}"
									  telerik:StyleManager.Theme="{StaticResource Theme}"
									  Padding="0">
                            <ItemsPresenter x:Name="presenter" Width="{TemplateBinding ViewportWidth}" HorizontalAlignment="Left" />
                        </ScrollViewer>
                    </Grid>
