	<Style TargetType="ScrollBar" x:Key="RibbonScrollViewerScrollBarStyle">
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="ScrollBar">
					<Grid x:Name="Root">
						

						<!-- Horizontal Template -->
						<Grid x:Name="HorizontalRoot">
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="Auto" />
							</Grid.ColumnDefinitions>

							<!-- Repeat Buttons + Thumb -->
							<RepeatButton  Grid.Column="0"
									Margin="-2 -2 -2 -1" Width="12" IsTabStop="False" Interval="50"
									Template="{StaticResource RibbonButtonTemplateHorizontal}"
									Background="#FFFFFFFF"
									BorderBrush="{StaticResource BasicBrush}"
									BorderThickness="1"
										   
									
								
									
									x:Name="DecreaseRepeat"
									Command="ScrollBar.LineLeftCommand"
									
								>
								<Path Stretch="Uniform" Height="5" Width="3"
										Data="F1 M 110.692,342.252L 110.692,352.682L 104.594,347.467L 110.692,342.252 Z"
										Fill="{StaticResource StrongBrush}" />
							</RepeatButton>
							<RepeatButton
									RenderTransformOrigin="0.5,0.5" Grid.Column="2"
									Margin="-2 -2 -2 -1" Width="12" IsTabStop="False" Interval="50"
									Template="{StaticResource RibbonButtonTemplateHorizontal}"
									Background="#FFFFFFFF"
									BorderBrush="{StaticResource BasicBrush}"
									BorderThickness="1" 
									
									

									
									x:Name="IncreaseRepeat"
									Command="ScrollBar.LineRightCommand"
									

								>
								<RepeatButton.RenderTransform>
									<ScaleTransform ScaleX="-1" />
								</RepeatButton.RenderTransform>
								<Path Stretch="Uniform" Height="5" Width="3"
										Data="F1 M 110.692,342.252L 110.692,352.682L 104.594,347.467L 110.692,342.252 Z"
										Fill="{StaticResource StrongBrush}" />
							</RepeatButton>
						</Grid>

						<!-- Vertical Template -->
						<Grid x:Name="VerticalRoot" Visibility="Collapsed">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto" />
								<RowDefinition Height="*" />
								<RowDefinition Height="Auto" />
							</Grid.RowDefinitions>

							<!-- Repeat Buttons + Thumb -->
							<RepeatButton x:Name="VerticalLargeDecrease" Grid.Row="0" Height="12"
									Margin="-2" Interval="50" IsTabStop="False"
									Template="{StaticResource RibbonButtonTemplateVertical}"
									Background="#FFFFFFFF"
									BorderBrush="{StaticResource BasicBrush}"
									BorderThickness="1">
								<Path Stretch="Uniform" Height="3" Width="5"
										Data="F1 M 541.537,173.589L 531.107,173.589L 536.322,167.49L 541.537,173.589 Z"
										Fill="{StaticResource StrongBrush}" />
							</RepeatButton>
							<RepeatButton x:Name="VerticalLargeIncrease" Grid.Row="2" Height="12"
									Margin="-2" Interval="50" IsTabStop="False"
									Template="{StaticResource RibbonButtonTemplateVertical}"
									Background="#FFFFFFFF"
									BorderBrush="{StaticResource BasicBrush}"
									BorderThickness="1">
								<Path Stretch="Uniform" Height="3" Width="5"
										Data="F1 M 531.107,321.943L 541.537,321.943L 536.322,328.042L 531.107,321.943 Z"
										Fill="{StaticResource StrongBrush}" />
							</RepeatButton>
						</Grid>
					</Grid>
					
					<ControlTemplate.Triggers>
						<Trigger Property="IsEnabled" Value="False">
							<Setter TargetName="Root" Property="Opacity" Value="0.5" />
						</Trigger>
					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	

	<Style TargetType="telerikRibbonBarPrimitives:RibbonScrollViewer">
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="ScrollViewerStyle">
			<Setter.Value>
				<Style TargetType="ScrollViewer">
					<Setter Property="BorderThickness" Value="0" />
					<Setter Property="IsTabStop" Value="False" />
					<Setter Property="Padding" Value="0" />
					<Setter Property="Template">
						<Setter.Value>
							<ControlTemplate TargetType="ScrollViewer">
								<Border BorderBrush="{TemplateBinding BorderBrush}"
										BorderThickness="{TemplateBinding BorderThickness}">
									<Grid Background="{TemplateBinding Background}">
										
										<ScrollContentPresenter x:Name="ScrollContentPresenter"
												Cursor="{TemplateBinding Cursor}"
												Margin="{TemplateBinding Padding}"
												ContentTemplate="{TemplateBinding ContentTemplate}"
												
												Content="{TemplateBinding Content}"
												CanContentScroll="{TemplateBinding CanContentScroll}"
												 />
											
										
										<ScrollBar
												Width="Auto"
												Style="{StaticResource RibbonScrollViewerScrollBarStyle}"
												Orientation="Vertical"
												ViewportSize="{TemplateBinding ViewportHeight}"
												Maximum="{TemplateBinding ScrollableHeight}"
												Minimum="0" IsTabStop="False"
												
												
												x:Name="PART_VerticalScrollBar"
												Value="{Binding VerticalOffset, Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}"
												Visibility="{TemplateBinding ComputedVerticalScrollBarVisibility}"
												
											/>
											
										
										<ScrollBar
												Height="Auto"
												Style="{StaticResource RibbonScrollViewerScrollBarStyle}"
												Orientation="Horizontal"
												ViewportSize="{TemplateBinding ViewportWidth}"
												Maximum="{TemplateBinding ScrollableWidth}"
												Minimum="0"
												IsTabStop="False"
													
												
												x:Name="PART_HorizontalScrollBar"
												Visibility="{TemplateBinding ComputedHorizontalScrollBarVisibility}"
												Value="{Binding HorizontalOffset, Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}"
												
											/>
										
									</Grid>
								</Border>
							</ControlTemplate>
						</Setter.Value>
					</Setter>
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBarPrimitives:RibbonScrollViewer">
					<Grid>
						
						<ScrollViewer Name="ScrollViewer"
								Style="{TemplateBinding ScrollViewerStyle}"
								HorizontalScrollBarVisibility="Auto"
								VerticalScrollBarVisibility="Disabled" IsTabStop="False">
							<ScrollViewer.Content>
								<ContentControl Name="Content" Content="{TemplateBinding Content}"
										IsTabStop="False" HorizontalContentAlignment="Stretch"
										VerticalContentAlignment="Stretch" />
							</ScrollViewer.Content>
						</ScrollViewer>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
    

    <Style TargetType="telerikRibbonBar:ScreenTip">
        <Setter Property="Padding" Value="4,2,4,2" />
        <Setter Property="Background" Value="#FFFFFFFF" />
        <Setter Property="Foreground" Value="{StaticResource StrongBrush}" />
        <Setter Property="BorderBrush" Value="{StaticResource BasicBrush}" />
        <Setter Property="MinHeight" Value="50" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:ScreenTip">
                    <Grid x:Name="RootElement">
                        
                        <Border Name="Border" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" >
                            <Grid Margin="{TemplateBinding Padding}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <!-- Text -->
                                <TextBlock Name="TitlePresenter" TextWrapping="Wrap" FontWeight="Bold" Grid.ColumnSpan="2" Text="{TemplateBinding Title}"/>

                                <!-- Image -->
                                <Image Name="Image" Grid.Row="1" Source="{TemplateBinding Icon}" Stretch="None" Margin="29,10,21,6" VerticalAlignment="Top" Visibility="Collapsed" />

                                <!-- Description -->
                                <TextBlock TextWrapping="Wrap"  Name="DescriptionPresenter" Grid.Row="1" Grid.Column="1" Text="{TemplateBinding Description}" VerticalAlignment="Top" Visibility="Collapsed" Margin="8,7,8,2" />
                            </Grid>
                        </Border>
                    </Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="ScreenTipState" Value="TitleDescriptionImage">
                            <Setter TargetName="DescriptionPresenter" Property="Visibility" Value="Visible" />
                            <Setter TargetName="TitlePresenter" Property="Margin" Value="0,4,0,0" />
                            <Setter TargetName="Image" Property="Visibility" Value="Visible" />
                            <Setter TargetName="RootElement" Property="Width" Value="318" />
                        </Trigger>
                        <Trigger Property="ScreenTipState" Value="TitleDescription">
                            <Setter TargetName="DescriptionPresenter" Property="Visibility" Value="Visible" />
                            <Setter TargetName="TitlePresenter" Property="Margin" Value="0,4,0,0" />
                            <Setter TargetName="RootElement" Property="Width" Value="210" />

                        </Trigger>
                        <Trigger Property="ScreenTipState" Value="Title">
                            <Setter TargetName="RootElement" Property="Width" Value="210" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

	
    

